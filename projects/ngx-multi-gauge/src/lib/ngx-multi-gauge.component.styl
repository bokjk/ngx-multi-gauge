.chart_container
  width : 500px
  height: 500px
  --border : 1px solid #ddd
  display : flex
  flex-direction : row
  justify-content: center
  align-items: center
  position : relative
  font-size: 14px

  .chart1
    display : flex


  .circle_text
    width : 100%
    height : 100%
    display : flex
    flex-direction : column
    justify-content : center
    align-items : center
    position : absolute
    font-size : 0.8em
    font-weight : bold



